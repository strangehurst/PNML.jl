<?xml version="1.0"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
    <net id="net0" type="pnmlcore">
        <name> <text>pnmlcore with nested pages</text> </name>
        <page id="page1">
            <place id="p1">
	        <name><text>place name</text></name>
	        <initialMarking> <text> 1 </text> </initialMarking>
	    </place>
            <transition id ="t1"> </transition>
            <arc id="a11" source="p1" target="t1"> <inscription> <text> 2 </text> </inscription> </arc>
            <arc id="a12" source="t1" target="rp1"> </arc>
            <arc id="a13" source="p1" target="rt1"> </arc>
            <referencePlace id="rp1" ref="p2"/>
            <referenceTransition id="rt1" ref="t11"/>
            <page id="page11">
                <transition id ="t11"> </transition>
		<arc id="a111" source="p11" target="rp11"> </arc>
		<referencePlace id="rp11" ref="p111"/>
                <page id="page111">
                    <place id="p111"> <initialMarking> <text> 111 </text> </initialMarking> </place>
		    <arc id="a1111" source="p111" target="rt111"> </arc>
		    <referenceTransition id="rt111" ref="rt2"/>
                </page>
            </page>
        </page>
        <page id="page2">
            <place id="p2"></place>
            <transition id ="t2"></transition>
            <arc id="a21" source="p2" target="t2"></arc>
            <arc id="a22" source="p2" target="rt2"></arc>
            <arc id="a23" source="t2" target="rp2"></arc>
            <referencePlace id="rp2" ref="p3"/>
            <referenceTransition id="rt2" ref="t3"/>
        </page>
        <page id="page3">
            <place id="p3"/>
            <transition id ="t3"/>
            <arc id="a31" source="p3" target="t3"/>
            <arc id="a32" source="t3" target="rp3"/>
            <referencePlace id="rp3" ref="p1"/>
        </page>
    </net>
    <net id="net1" type="http://www.pnml.org/version-2009/grammar/ptnet">
        <page id="net1page"/>
    </net>
    <net id="net2" type="ptnet"> <page id="net2page"/> </net>
    <net id="net3" type="hlcore"> <page id="net3page"/> </net>
    <net id="net4" type="hlnet"> <page id="net4page"/> </net>
    <net id="net5" type="pt_hlpng"> <page id="net5page"/> </net>
    <net id="net6" type="symmetric"> <page id="net6page"/> </net>
    <net id="net7" type="timednet"> <page id="net7page"/> </net>
    <net id="net8" type="continuous"> <page id="net8page"/> </net>
    <net id="net9" type="bogus"> <page id="net9page"/> </net>
</pnml>
